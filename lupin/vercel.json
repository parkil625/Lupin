{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.lupin-care.com/api/:path*"
    },
    {
      "source": "/ws/:path*",
      "destination": "https://api.lupin-care.com/ws/:path*"
    },
    {
      "source": "/((?!api|ws|_next|static|[\\w-]+\\.\\w+).*)",
      "destination": "/index.html"
    }
  ]
}
